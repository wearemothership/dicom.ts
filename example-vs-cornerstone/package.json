{
	"name": "dicom.ts-example-vs-cornerstone",
	"homepage": ".",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"postinstall": "rimraf ./public/images && cp -r ../node_modules/dicom-test-files/vpop-pro.com ./public/images"
	},
	"dependencies": {
		"browserify-fs": "^1.0.0",
		"cornerstone-core": "^2.6.1",
		"cornerstone-file-image-loader": "^0.3.0",
		"cornerstone-wado-image-loader": "git+https://github.com/webnamics/cornerstoneWADOImageLoader.git",
		"dicom-parser": "^1.8.21",
		"dicom.ts": "file:..",
		"path-browserify": "^1.0.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-icons": "^5.2.1",
		"react-router-dom": "^6.23.1",
		"stream-browserify": "^3.0.0"
	},
	"devDependencies": {
		"@types/cornerstone-core": "^2.3.3",
		"@types/react": "^18.3.18",
		"@types/react-dom": "^18.3.5",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"@vitejs/plugin-react": "^4.2.1",
		"eslint": "^8.56.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"rimraf": "^5.0.5",
		"typescript": "^5.7.3",
		"vite": "^5.1.0"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"jest": {
		"moduleNameMapper": {
			"^dicom.ts$": "<rootDir>/../dist/index.modern.js"
		}
	},
	"browserslist": {
		"production": [
			">1.0%",
			"not ie 11",
			"not dead",
			"not op_mini all"
		],
		"development": [
			">1.0%",
			"not ie 11",
			"not dead",
			"not op_mini all"
		]
	}
}
